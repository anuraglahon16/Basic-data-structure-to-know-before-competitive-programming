#include <iostream>
#include <string>
#include <math.h>
#include <vector>
#include <cstring>
#include <algorithm>
using namespace std;
int main() {
	//code
	int t;
	string s,str;
	cin>>t;
	while(t--){
	    cin>>s;
	    cin>>str;
	    int pos = s.find(str);
	    if(pos==-1)cout<<"not found"<<endl;
	    else cout<<"found"<<endl;
	}
	return 0;
}
